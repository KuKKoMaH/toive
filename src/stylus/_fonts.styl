@font-face {
  font-family: 'Playfair Display';
  src: url('../fonts/hinted-subset-PlayfairDisplay-Black.eot');
  src: url('../fonts/hinted-subset-PlayfairDisplay-Black.eot?#iefix') format('embedded-opentype'),
          url('../fonts/hinted-subset-PlayfairDisplay-Black.woff2') format('woff2'),
          url('../fonts/hinted-subset-PlayfairDisplay-Black.woff') format('woff'),
          url('../fonts/hinted-subset-PlayfairDisplay-Black.ttf') format('truetype');
  font-weight: 900;
  font-style: normal;
}

@font-face {
  font-family: 'Playfair Display';
  src: url('../fonts/hinted-subset-PlayfairDisplay-Bold.eot');
