          url('../fonts/hinted-subset-PlayfairDisplay-Bold.woff2') format('woff2'),
          url('../fonts/hinted-subset-PlayfairDisplay-Bold.woff') format('woff'),
          url('../fonts/hinted-subset-PlayfairDisplay-Bold.ttf') format('truetype');
  font-weight: bold;
  font-style: normal;
}

@font-face {
